X					return (handle_timer(c, w));
X				}
X
X				play_next_entry();
X				if (cd_status() != 1)
X					return (handle_timer(c, w));
X				if (cur_cdmode == 4)	/* Done with CD */
X				{
X					xv_set(Workman_goodies->abrepeat,
