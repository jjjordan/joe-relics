X				pixrow[17][maxcol] || pixrow[18][maxcol] || 
X				pixrow[19][maxcol] || pixrow[20][maxcol] || 
X				pixrow[21][maxcol] || pixrow[22][maxcol] || 
X				pixrow[23][maxcol])
X			    break;
X
X		if (maxcol >= 0)
X		{
X			++maxcol;
X			printf("\033*'");
X			putc((maxcol*8)%256,stdout);
X			putc((maxcol*8)/256,stdout);
X			for (j=0; j<=maxcol; j++)
X			{
X				for (k=0; k<8; k++)
X				{
X					a = '\0';
X					for (i=0; i<8; i++)
X						if (pixrow[i][j] & bitval[k])
X							a |= bitval[i];
X					putc(a,stdout);
X
X					a = '\0';
X					for (; i<16; i++)
X						if (pixrow[i][j] & bitval[k])
