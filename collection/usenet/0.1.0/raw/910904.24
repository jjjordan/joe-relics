X#define	min(a,b)	((a<b)?a:b)
X#define	max(a,b)	((a>b)?a:b)
X#endif
X
Xunsigned char io[512];
Xunsigned char pixrow[24][512];
Xunsigned char bitval[]=
X{
X	0x80, 0x40, 0x20, 0x10, 0x08, 0x04, 0x02, 0x01,
X	0x80, 0x40, 0x20, 0x10, 0x08, 0x04, 0x02, 0x01,
X	0x80, 0x40, 0x20, 0x10, 0x08, 0x04, 0x02, 0x01
X};
X
Xdie(va_alist)
Xva_dcl
X{
X	va_list args;
X	char *fmt;
X
X	va_start(args);
X	fmt = va_arg(args, char *);
X	fprintf(stderr,"pbmtoe24: ");
X	vfprintf(stderr, fmt, args);
X	va_end(args);
X	exit(1);
X}
Xmain(ac,av)
Xint ac;
Xchar *av[];
X{
X	FILE *in;
X	int cols, rows, row, eof, i, j, k, maxcol;
X	unsigned char a;
X	char *fname;
X
X	in = NULL;
X	if (ac == 2)
X	{
X		if ((in = fopen(av[1], "r")) == NULL)
X			die("can't open %s\n",av[1]);
X		fname = av[1];
X	}
X	if (in == NULL)
X	{
X		if (isatty(fileno(stdin)))
X			die(" need infile [or stdin]\n");
X		in = stdin;
X		fname = "(stdin)";
X	}
X	if (fgets((char*)io, 20, in) == NULL || strncmp(io, "P4", 2))
X		die("%s doesn't look like a pbm file\n",fname);
X	for (;;)
X	{
X		if (fgets((char*)io, 256, in) == NULL)
X			die("%s doesn't look like a pbm file\n",fname);
X		if (io[0] != '#')
X		{
X			if (sscanf(io, "%d %d",&cols,&rows) != 2)
X				die("%s doesn't look like a pbm file\n",fname);
X			break;
X		}
X	}
X	if (!(cols % 8))
X		cols /= 8;
X	else cols = 1+(cols/8);
X
X	printf("\033@\0333\030");
X	/* ESC @ to sane printer */
X	/* ESC 3 \030 24/180 line spacing */
X
X	for (eof=row=0; ; )
X	{
X		for (i=0; i<24 && row<rows; i++, row++)
X		{
X			if (fread(pixrow[i], cols, 1, in) != 1)
X			{
X				eof = TRUE;
X				break;
X			}
X		}
X		if (i < 24)
X		{
X			for (; i<24; i++)
X				memset(pixrow[i], '\0', cols);
X			eof = TRUE;
X		}
X						/* trim white space */
X		for (maxcol = cols-1; maxcol >= 0; --maxcol)
X			if (pixrow[0][maxcol] || 
X				pixrow[1][maxcol] || pixrow[2][maxcol] || 
X				pixrow[3][maxcol] || pixrow[4][maxcol] || 
X				pixrow[5][maxcol] || pixrow[6][maxcol] || 
X				pixrow[7][maxcol] || pixrow[8][maxcol] || 
X				pixrow[9][maxcol] || pixrow[10][maxcol] || 
X				pixrow[11][maxcol] || pixrow[12][maxcol] || 
